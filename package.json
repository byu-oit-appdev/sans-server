{
  "name": "sans-server",
  "version": "1.5.0",
  "description": "Write code for a server, without the server.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/*.js",
    "coverage": "nyc --reporter=html mocha test/*.js",
    "coverage:report": "nyc mocha test/*.js && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "server",
    "serverless",
    "connect",
    "request",
    "response",
    "middleware"
  ],
  "author": "James Speirs <james.speirs@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "fully-typed": "^1.1.2",
    "http-status": "^0.2.3",
    "sans-server-middleware": "^1.2.3",
    "uuid": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/sans-server.git"
  },
  "bugs": {
    "url": "https://github.com/byu-oit/sans-server/issues"
  },
  "homepage": "https://github.com/byu-oit/sans-server#readme"
}
